  <doap:Project>
    <doap:name>Raptor</doap:name>
    <doap:summary xml:lang="en">Raptor RDF Parser Toolkit Library.</doap:summary>

    <doap:description xml:lang="en">Raptor is the RDF Parser Toolkit for
    Redland that provides a set of Resource Description Framework (RDF)
    parsers and serializers, generating RDF triples from RDF/XML,
    N-Triples, Turtle, several XML RSS tag soup formats and Atom 0.3
    along with serializers to RDF/XML and N-Triples.</doap:description>

    <doap:homepage rdf:resource="http://librdf.org/raptor/"/>

    <doap:maintainer rdf:nodeID="p1"/>

    <doap:download rdf:resource="http://librdf.org/dist/sources/"/>

